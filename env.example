# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/contract_ai"

# OpenAI Configuration
OPENAI_API_KEY="your-openai-api-key"
OPENAI_MODEL="gpt-4"

# Backend Configuration
BACKEND_PORT=3001
BACKEND_URL="http://localhost:3001"
NODE_ENV="development"

# Frontend Configuration
NEXT_PUBLIC_BACKEND_URL="http://localhost:3001"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Voice Configuration
JAMBONZ_ACCOUNT_SID="your-jambonz-account-sid"
JAMBONZ_API_KEY="your-jambonz-api-key"
JAMBONZ_API_SECRET="your-jambonz-api-secret"
JAMBONZ_WEBHOOK_SECRET="your-webhook-secret"

# Whisper ASR Configuration
WHISPER_API_URL="http://localhost:9000/asr"
WHISPER_MODEL="base"

# Coqui TTS Configuration
COQUI_TTS_URL="http://localhost:5002/api/tts"
COQUI_VOICE="en_0"

# JWT Configuration (for session management)
JWT_SECRET="your-jwt-secret-key"
JWT_EXPIRES_IN="7d"

